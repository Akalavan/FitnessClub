<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="145.0"
      prefWidth="248.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.controllers.chart.ChoiceChartController">
    <children>
        <HBox prefHeight="39.0" prefWidth="248.0">
            <children>
                <ChoiceBox fx:id="choiceRep" prefWidth="150.0">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </ChoiceBox>
                <Label text="Кого">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="7.0"/>
                    </HBox.margin>
                </Label>
            </children>
        </HBox>
        <HBox prefHeight="39.0" prefWidth="317.0">
            <children>
                <DatePicker fx:id="dateStart">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </DatePicker>
                <Label text="С какого">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="7.0"/>
                    </HBox.margin>
                </Label>
            </children>
        </HBox>
        <HBox prefHeight="35.0" prefWidth="248.0">
            <children>
                <DatePicker fx:id="dateEnd">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </DatePicker>
                <Label text="По какое">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="7.0"/>
                    </HBox.margin>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_RIGHT" prefHeight="29.0" prefWidth="248.0">
            <children>
                <Button fx:id="btnOk" defaultButton="true" mnemonicParsing="false" onAction="#actionButtonPressed"
                        text="Отчёт">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</VBox>

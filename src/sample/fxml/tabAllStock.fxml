<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.controllers.TableAllStockController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="tableAllStock" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="columnId" prefWidth="75.0" text="id"/>
                <TableColumn fx:id="dateStart" minWidth="1.0" prefWidth="78.0" text="Старт акции"/>
                <TableColumn fx:id="dateEnd" prefWidth="128.65570068359375" text="Окончания окации"/>
                <TableColumn fx:id="condition" minWidth="0.0" prefWidth="115.36956787109375" text="Условие"/>
                <TableColumn fx:id="comment" prefWidth="155.0" text="Комментарий"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>

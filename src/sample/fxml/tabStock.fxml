<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.TableStockController">
    <children>
        <TableView fx:id="tableStock" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="columnId" prefWidth="75.0" text="id"/>
                <TableColumn fx:id="stock" minWidth="1.0" prefWidth="78.0" text="Акция"/>
                <TableColumn fx:id="columnName" prefWidth="128.65570068359375" text="Имя"/>
                <TableColumn fx:id="columnSurname" minWidth="0.0" prefWidth="115.36956787109375" text="Фамилия"/>
                <TableColumn fx:id="columnPatronymic" prefWidth="155.0" text="Отчество"/>
                <TableColumn fx:id="columnEmail" prefWidth="155.0" text="Маил"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>